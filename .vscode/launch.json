  "version": "0.2.0",
  "configurations": [
    {
      "name": "trainYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "configs/yolov5/yolov5_s-p6-v62_syncbn_fast_8xb16-10e_asdr6-2-100-autosplit_320_anchOptm_testJsonLog.py"
    },
    {
      "name": "testYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/test.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_TEST": "true"
      },
      "args": "configs/yolov5/${config:MMYOLO_CONFIG}.py models/${config:MMYOLO_MODEL}.pth --out outputs/${config:MMYOLO_OUTPUT}/result.pkl"
    },
    {
      "name": "confusionMatrixYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/confusion_matrix.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "configs/yolov5/${config:MMYOLO_CONFIG}.py outputs/${config:MMYOLO_MODEL}/result.pkl outputs/${config:MMYOLO_OUTPUT}"
    },
    {
      "name": "inference-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/asdr/inference.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "--threshold=0.3 --config=configs/yolov5/${config:MMYOLO_CONFIG}.py --checkpoint models/${config:MMYOLO_MODEL}.pth --image=data/asdr6_2_100_autosplit/inferences/utg-UThong-m-mt.jpg"
    }
  ]
}
