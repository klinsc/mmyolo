{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "trainYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_TEST": "true"
      },
      "args": "configs/rtmdet/rtmdet_tiny_fast_1xb12-40e_cat.py"
    },
    {
      "name": "testYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/test.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_TEST": "true"
      },
      "args": "configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py models/${config:MMYOLO_MODEL}.pth --out outputs/${config:MMYOLO_OUTPUT}/result.pkl"
    },
    {
      "name": "confusionMatrixYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/confusion_matrix.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_TEST": "true"
      },
      "args": "configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py outputs/${config:MMYOLO_MODEL}/result.pkl outputs/${config:MMYOLO_OUTPUT}"
    },
    {
      "name": "inference-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/asdr/inference.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_OUTPUT": "${config:MMYOLO_OUTPUT}",
        "MMYOLO_TEST": "true"
      },
      "args": "--threshold=0.5 --config=configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py --checkpoint models/${config:MMYOLO_MODEL}.pth --image=${config:MMYOLO_IMAGE}"
    },
    {
      "name": "dataset_analysis:train",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/dataset_analysis.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "--out-dir ./dataset_analysis/${config:MMYOLO_CONFIG}/train configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py"
    },
    {
      "name": "dataset_analysis:val",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/dataset_analysis.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "--val-dataset --out-dir ./dataset_analysis/${config:MMYOLO_CONFIG}/val configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py"
    },
    {
      "name": "browse_coco_json-train",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_OUTPUT": "${config:MMYOLO_OUTPUT}"
      },
      "program": "tools/analysis_tools/browse_coco_json.py",
      "args": "--img-dir data/${config:MMYOLO_DATASET}/images --ann-file data/${config:MMYOLO_DATASET}/annotations/train.json --shuffle --wait-time 100 --category-names 115_1way_ds_w_motor"
    },
    {
      "name": "browe_coco_json:val",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/browse_coco_json.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "--img-dir data/${config:MMYOLO_DATASET}/images --ann-file data/${config:MMYOLO_DATASET}/annotations/val.json --shuffle --wait-time 10 --category-names 115_1way_ds_w_motor"
    }
  ]
}
