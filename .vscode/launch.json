{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "trainYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_TEST": "true"
      },
      "args": "configs/rtmdet/rtmdet_tiny_fast_1xb12-40e_cat.py"
    },
    {
      "name": "testYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/test.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_TEST": "true"
      },
      "args": "configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py models/${config:MMYOLO_MODEL}.pth --out outputs/${config:MMYOLO_OUTPUT}/result.pkl"
    },
    {
      "name": "confusionMatrixYOLO-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/analysis_tools/confusion_matrix.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": "configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py outputs/${config:MMYOLO_MODEL}/result.pkl outputs/${config:MMYOLO_OUTPUT}"
    },
    {
      "name": "inference-ASDR",
      "type": "python",
      "request": "launch",
      "program": "tools/asdr/inference.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "MMYOLO_OUTPUT": "${config:MMYOLO_OUTPUT}"
      },
      "args": "--threshold=0.3 --config=configs/${config:MMYOLO_PARENT}/${config:MMYOLO_CONFIG}.py --checkpoint models/${config:MMYOLO_MODEL}.pth --image=${config:MMYOLO_IMAGE}"
    }
  ]
}
